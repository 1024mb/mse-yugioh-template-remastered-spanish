
############################################################## Card pack items

pack type:
	name: rara
	filter: card.rareza != "común"
pack type:
	name: común
	filter: card.rareza == "común"
pack type:
	name: dorada
	filter: card.rareza == "tec. dorada"
pack type:
	name: card
	filter: card.número != ""

